{
  "name": "tech-radar-backend",
  "version": "1.0.0",
  "description": "Backend для Tech Radar приложения",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d typeorm.config.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d typeorm.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d typeorm.config.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d typeorm.config.ts",
    "seed": "ts-node src/database/seed.ts"
  },
  "keywords": [
    "tech-radar",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "express-session": "^1.19.0",
    "jsonwebtoken": "^9.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.19.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.3.2",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "nodemon": "^3.1.14",
    "typeorm-cli": "^1.0.7",
    "typescript": "^5.9.3"
  }
}
